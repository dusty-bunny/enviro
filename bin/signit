#!/bin/bash


export SIGN_LOGFILE=/tmp/log.signit

dbg_echo()
{
        echo -e "$*" |& tee -a $SIGN_LOGFILE
}
dbg_echo "......................"

gpg --clear-sign Release.dinstall-new |& tee -a $SIGN_LOGFILE
if [ $? -eq 0 ] ; then
        dbg_echo "========================="
        gpg --no-tty -abs Release.dinstall-new
        mv Release.dinstall-new.asc Release.gpg
#       cp Release.dinstall-new.asc Release.dinstall-new
fi

dbg_echo ",,,,,,,,,,,,,,,,,,,,,,,,,"
exit 0
