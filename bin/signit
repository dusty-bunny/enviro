#!/bin/bash


export SIGN_LOGFILE=/tmp/log.signit

dbg_echo()
{
        echo -e "$*" |& tee -a $SIGN_LOGFILE
}
dbg_echo "......................"

gpg --clearsign Release.dinstall-new |& tee -a $SIGN_LOGFILE
if [ $? -eq 0 ] ; then
        dbg_echo "========================="
fi

dbg_echo ",,,,,,,,,,,,,,,,,,,,,,,,,"
exit 0
