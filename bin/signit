#!/bin/bash

[ ! -f "$1" ] && exit 5
echo "Sign $1"
if gpg --verbose --clearsign -o InRelease.xxx $1 ; then
        mv InRelease.xxx InRelease
        echo "Create gpg file."
        if gpg -abs -o Release.gpg.xxx $1 ; then
                mv Release.gpg.xxx Release.gpg
        fi
else
        echo "Failed."
fi
