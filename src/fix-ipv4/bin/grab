#!/home/zglue/src/fix-ipv4/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'grab','console_scripts','grab'
__requires__ = 'grab'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('grab', 'console_scripts', 'grab')()
    )
